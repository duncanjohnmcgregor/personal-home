# Staging Environment Configuration
# Copy this file and fill in actual values for staging deployment

# Database URLs (PostgreSQL) - Staging Database
POSTGRES_URL="postgresql://user:password@staging-host:5432/staging-database?sslmode=require"
POSTGRES_PRISMA_URL="postgresql://user:password@staging-host:5432/staging-database?sslmode=require&pgbouncer=true"
POSTGRES_URL_NON_POOLING="postgresql://user:password@staging-host:5432/staging-database?sslmode=require"

# NextAuth Configuration - Staging
NEXTAUTH_URL="https://staging.your-domain.com"
NEXTAUTH_SECRET="your-staging-nextauth-secret"

# Spotify OAuth Configuration - Staging App
SPOTIFY_CLIENT_ID="your-staging-spotify-client-id"
SPOTIFY_CLIENT_SECRET="your-staging-spotify-client-secret"

# Beatport API Configuration - Staging
BEATPORT_API_TOKEN="your-staging-beatport-api-token"

# Application Configuration
NODE_ENV="staging"

# Monitoring and Analytics
SENTRY_DSN="your-staging-sentry-dsn"
VERCEL_ANALYTICS_ID="your-staging-vercel-analytics-id"

# Feature Flags
ENABLE_BEATPORT_PURCHASE="false"
ENABLE_SOUNDCLOUD_INTEGRATION="false"