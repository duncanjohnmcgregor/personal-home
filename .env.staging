# Staging Environment Configuration
# Copy this file and rename to .env.local for staging deployment

# Application
NEXTAUTH_URL=https://music-playlist-manager-staging.vercel.app
NEXTAUTH_SECRET=staging-secret-key-change-this

# Database (Use staging database)
DATABASE_URL=postgresql://username:password@staging-host:5432/music_playlist_manager_staging

# Spotify API (Use staging app credentials)
SPOTIFY_CLIENT_ID=staging-spotify-client-id
SPOTIFY_CLIENT_SECRET=staging-spotify-client-secret

# Optional: Additional staging configurations
NODE_ENV=staging
VERCEL_URL=https://music-playlist-manager-staging.vercel.app

# Monitoring and Analytics (Optional)
SENTRY_DSN=your-staging-sentry-dsn
VERCEL_ANALYTICS_ID=your-staging-analytics-id